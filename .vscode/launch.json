{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: BasicCNN with MNIST (model 1)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": ["--dir=results/",
                "--dataset=mnist",
                "--data-path=datasets/",
                "--model=CNN",
                "--epochs=5",
                "--lr=0.01",
                "--wd=5e-4",
                "--transform=CNN",
                "--seed=1"]
        }]
}